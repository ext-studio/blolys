{
  "name": "blolys",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "lint": "ng lint",
    "http": "http-server dist",
    "prebuild-i18n": "ng xi18n --output-path client/i18n",
    "build:all": "yarn build && yarn build-i18n:cn",
    "build-i18n:cn": "ng build --output-path=dist/cn --aot -prod -vc=true --bh /cn/ --i18n-file=client/i18n/messages.cn.xlf --i18n-format=xlf --locale=zh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "http-server": "^0.11.1",
    "ngtouchstart": "^1.0.1",
    "protractor": "^5.3.2",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
