{
	"name": "blolys",
	"version": "0.0.1",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build -prod --aot -prod -vc=true",
		"lint": "ng lint",
		"http": "http-server dist",
		"prod": "yarn build && go run main.go",
		"build:all": "yarn i18n:default && yarn i18n:cn && yarn i18n:en",
		"i18n:pre": "ng xi18n --output-path client/i18n",
		"i18n:default": "yarn build --output-path=dist --i18n-file=client/i18n/messages.cn.xlf --i18n-format=xlf --locale=zh",
		"i18n:cn": "yarn build --output-path=dist/cn --bh /cn/ --i18n-file=client/i18n/messages.cn.xlf --i18n-format=xlf --locale=zh",
		"i18n:en": "yarn build --output-path=dist/en --bh /en/",
		"preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Please use Yarn instead of NPM to install dependencies. See: https://yarnpkg.com/lang/en/docs/install/')\""
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^5.2.0",
		"@angular/cdk": "^5.2.4",
		"@angular/common": "^5.2.0",
		"@angular/compiler": "^5.2.0",
		"@angular/core": "^5.2.0",
		"@angular/forms": "^5.2.0",
		"@angular/http": "^5.2.0",
		"@angular/material": "^5.2.4",
		"@angular/platform-browser": "^5.2.0",
		"@angular/platform-browser-dynamic": "^5.2.0",
		"@angular/router": "^5.2.0",
		"core-js": "^2.4.1",
		"http-server": "^0.11.1",
		"ngtouchstart": "^1.0.1",
		"protractor": "^5.3.2",
		"rxjs": "^5.5.6",
		"zone.js": "^0.8.19"
	},
	"devDependencies": {
		"@angular/cli": "~1.7.4",
		"@angular/compiler-cli": "^5.2.0",
		"@angular/language-service": "^5.2.0",
		"@types/node": "~6.0.60",
		"codelyzer": "^4.0.1",
		"ts-node": "~4.1.0",
		"tslint": "~5.9.1",
		"typescript": ">=2.4.2 <2.7.0"
	}
}
